{
  "name": "open-telemetry/opentelemetry-auto-psr16",
  "description": "OpenTelemetry auto-instrumentation for PSR-16 (Common Interface for Caching Libraries).",
  "keywords": ["opentelemetry", "otel", "open-telemetry", "tracing", "psr16", "instrumentation"],
  "type": "library",
  "homepage": "https://opentelemetry.io/docs/php",
  "readme": "./README.md",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-opentelemetry": "*",
    "open-telemetry/api": "^1.0",
    "open-telemetry/sem-conv": "^1.24",
    "psr/simple-cache": "^1 || ^2 || ^3",
    "composer-runtime-api": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "phan/phan": "^5.0",
    "phpstan/phpstan-mockery": "^1.1.0",
    "phpstan/phpstan": "^1.1",
    "phpstan/phpstan-phpunit": "^1.0",
    "psalm/plugin-phpunit": "^0",
    "open-telemetry/sdk": "^1.0",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "OpenTelemetry\\Contrib\\Instrumentation\\Psr16\\": "src/"
    },
    "files": [
      "_register.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OpenTelemetry\\Tests\\Instrumentation\\Psr16\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
